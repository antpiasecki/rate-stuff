generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Movie {
  id          String  @id @default(cuid())
  title       String
  year        Int
  imdb_image  String?
  imdb_rating Float   @default(0)
  user_rating Int     @default(0)

  @@unique([title, year])
}
